<?xml version="1.0" encoding="UTF-8"?>
<process
    name="BookOrderingProcess"
    targetNamespace="http://enterprise.netbeans.org/bpel/lab2_25/BookOrderingProcess"
    xmlns:tns="http://enterprise.netbeans.org/bpel/lab2_25/BookOrderingProcess"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling">
    <import namespace="http://infosys.tuwien.ac.at/aic10/ass2/process" location="OrderProcess.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/customerserviceWrapper" location="customerserviceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/shippingserviceWrapper" location="shippingserviceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/supplier1Wrapper" location="http://localhost:8088/supplier1?wsdl=SupplierService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/supplier1Wrapper" location="supplier1Wrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="OrderProcess" xmlns:tns="http://infosys.tuwien.ac.at/aic10/ass2/process" partnerLinkType="tns:OrderProcessLinkType" myRole="OrderProcessProvider"/>
        <partnerLink name="CustomerService" xmlns:tns="http://enterprise.netbeans.org/bpel/customerserviceWrapper" partnerLinkType="tns:CustomerServiceLinkType" myRole="CustomerServiceRole"/>
        <partnerLink name="ShippingService" xmlns:tns="http://enterprise.netbeans.org/bpel/shippingserviceWrapper" partnerLinkType="tns:ShippingServiceLinkType" myRole="ShippingServiceRole"/>
    </partnerLinks>
    <variables>
        <variable name="Start_processIn" xmlns:tns="http://infosys.tuwien.ac.at/aic10/ass2/process" messageType="tns:start_process_request"/>
    </variables>
    <sequence name="Sequence1">
        <receive name="Receive1" createInstance="yes" partnerLink="OrderProcess" operation="start_process" xmlns:tns="http://infosys.tuwien.ac.at/aic10/ass2/process" portType="tns:OrderProcessPortType"/>
        <invoke name="Invoke1"/>
    </sequence>
</process>



