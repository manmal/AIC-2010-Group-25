<?xml version="1.0" encoding="UTF-8"?>
<process
    name="BookOrderingProcess"
    targetNamespace="http://enterprise.netbeans.org/bpel/lab2_25/BookOrderingProcess"
    xmlns:tns="http://enterprise.netbeans.org/bpel/lab2_25/BookOrderingProcess"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:ns4="http://services.aic2010/" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns5="http://infosys.tuwien.ac.at/aic10/ass2/OrderProcessSchema" xmlns:ns6="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:sxxf="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/XPathFunctions">
    <import namespace="http://infosys.tuwien.ac.at/aic10/ass2/process" location="OrderProcess.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/customerserviceWrapper" location="customerserviceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/warehouseWrapper" location="warehouseWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://services.aic2010/" location="http://localhost:8088/customerservice?wsdl=CustomerService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://services.aic2010/" location="http://localhost:8088/warehouse?wsdl=WarehouseService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/supplier2Wrapper" location="supplier2Wrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://services.aic2010/" location="http://localhost:8088/supplier2?wsdl=SupplierService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/supplier1Wrapper" location="supplier1Wrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://services.aic2010/" location="http://localhost:8088/supplier1?wsdl=SupplierService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="OrderProcess" xmlns:tns="http://infosys.tuwien.ac.at/aic10/ass2/process" partnerLinkType="tns:OrderProcessLinkType" myRole="OrderProcessProvider"/>
        <partnerLink name="CustomerService" xmlns:tns="http://enterprise.netbeans.org/bpel/customerserviceWrapper" partnerLinkType="tns:CustomerServiceLinkType" partnerRole="CustomerServiceRole"/>
        <partnerLink name="WarehouseService" xmlns:tns="http://enterprise.netbeans.org/bpel/warehouseWrapper" partnerLinkType="tns:WarehouseServiceLinkType" partnerRole="WarehouseServiceRole"/>
        <partnerLink name="Supplier1" xmlns:tns="http://enterprise.netbeans.org/bpel/supplier1Wrapper" partnerLinkType="tns:SupplierServiceLinkType" partnerRole="SupplierServiceRole"/>
        <partnerLink name="Supplier2" xmlns:tns="http://enterprise.netbeans.org/bpel/supplier2Wrapper" partnerLinkType="tns:SupplierServiceLinkType" partnerRole="SupplierServiceRole"/>
    </partnerLinks>
    <variables>
        <variable name="customerOut" messageType="ns4:get_customerResponse"/>
        <variable name="customerIn" messageType="ns4:get_customer"/>
        <variable name="processIn" xmlns:tns="http://infosys.tuwien.ac.at/aic10/ass2/process" messageType="tns:start_process_request"/>
    </variables>
    <sequence name="Sequence1">
        <receive name="ReceiveStartEvent" createInstance="yes" partnerLink="OrderProcess" operation="start_process" xmlns:tns="http://infosys.tuwien.ac.at/aic10/ass2/process" portType="tns:OrderProcessPortType" variable="processIn"/>
        <assign name="AssignCustomerID">
            <copy>
                <from>$processIn.part1/ns5:customer</from>
                <to>$customerIn.parameters/id</to>
            </copy>
        </assign>
        <invoke name="getCustomer" partnerLink="CustomerService" operation="get_customer" portType="ns4:CustomerService" inputVariable="customerIn" outputVariable="customerOut"/>
        <forEach name="ForEachOrderPart" parallel="no" counterName="ForEachPartCounter">
            <startCounterValue>1</startCounterValue>
            <finalCounterValue>count($processIn.part1/ns5:orderParts)</finalCounterValue>
            <scope name="OrderPart">
                <variables>
                    <variable name="OrderOut" messageType="ns4:orderResponse"/>
                    <variable name="OrderIn" messageType="ns4:order"/>
                    <variable name="warehouseOut" messageType="ns4:check_availabilityResponse"/>
                    <variable name="warehouseIn" messageType="ns4:check_availability"/>
                </variables>
                <sequence name="Sequence2">
                    <assign name="AssignProductIDAndAmount">
                        <copy>
                            <from>$processIn.part1/ns5:orderParts/ns5:product_id</from>
                            <to>$warehouseIn.product/@id</to>
                        </copy>
                        <copy>
                            <from>$processIn.part1/ns5:orderParts/ns5:amount</from>
                            <to variable="warehouseIn" part="amount"/>
                        </copy>
                    </assign>
                    <invoke name="CheckAvailability" partnerLink="WarehouseService" operation="check_availability" portType="ns4:WarehouseService" inputVariable="warehouseIn" outputVariable="warehouseOut"/>
                    <if name="IfAvailableinWarehouse">
                        <condition>$warehouseOut.return/isAvailable</condition>
                        <invoke name="OrderInWarehouse" partnerLink="WarehouseService" operation="order" portType="ns4:WarehouseService" inputVariable="OrderIn" outputVariable="OrderOut"/>
                        <else>
                            <invoke name="OrderInSupplier"/>
                        </else>
                    </if>
                </sequence>
            </scope>
        </forEach>
    </sequence>
</process>





